{
  "name": "plain-flags-management",
  "version": "1.0.0",
  "description": "Plain flags management service",
  "main": "bin/index.js",
  "scripts": {
    "test": "test",
    "build": "tsc -v && tsc -w",
    "build-prod": "tsc",
    "start": "node --watch bin/index.js",
    "start-prod": "NODE_ENV=production node bin/index.js",
    "dev": "env-cmd -f .env node --inspect=9229 --watch bin/index.js",
    "typeorm": "NODE_ENV=migration typeorm-ts-node-commonjs",
    "typeorm:generate": "npm run typeorm migration:generate -- --dataSource ./src/data.ts",
    "typeorm:create": "npm run typeorm migration:create -- --dataSource ./src/data.ts",
    "typeorm:migrate": "npm run typeorm migration:run -- --dataSource ./src/data.ts",
    "typeorm:revert": "npm run typeorm migration:revert -- --dataSource ./src/data.ts"
  },
  "author": "Andrei Leonte",
  "license": "ISC",
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/luxon": "^3.4.2",
    "@types/node": "^22.9.0",
    "fastify-plugin": "^5.0.1",
    "pino-pretty": "^13.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "@fastify/cors": "^10.0.1",
    "@fastify/jwt": "^9.0.1",
    "@types/ws": "^8.5.14",
    "bcrypt": "^5.1.1",
    "env-cmd": "^10.1.0",
    "fastify": "^5.1.0",
    "luxon": "^3.5.0",
    "sqlite3": "^5.1.7",
    "typeorm": "^0.3.20",
    "typeorm-naming-strategies": "^4.1.0",
    "upath": "^2.0.1",
    "ws": "^8.18.0"
  }
}
